{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "PathFormat": "/logs/application-{Date}.log"
  },
  "Upload": {
    "PathFormat": "/uploads/{Name}"
  },
  "Validation": {
    "ShellExecutable": "/bin/bash"
  }
}
