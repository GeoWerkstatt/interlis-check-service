{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "PathFormat": "Logs/application-{Date}.log",
    "PathFormatSession": "Logs/session-{SessionId}.log"
  },
  "Upload": {
    "PathFormat": "Upload/{FileName}"
  },
  "AllowedHosts": "*"
}
